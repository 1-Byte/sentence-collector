{
  "name": "sentence-collector",
  "version": "0.0.1",
  "description": "A web console for collecting and validating sentences",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist .cache node_modules yarn.lock",
    "reset": "yarn clean && yarn",
    "start": "rm -rf dist .cache && parcel web/index.html",
    "build": "parcel build --no-cache web/index.html --public-url .",
    "lint": "eslint shared web scripts",
    "init-db": "parcel build -t \"node\" scripts/init-db.js && node dist/init-db",
    "deploy": "yarn build && gh-pages -d dist",
    "test": "yarn lint && yarn build"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "kinto": "^11.2.0",
    "kinto-http": "^4.5.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "btoa": "^1.2.1",
    "eslint": "4.x",
    "gh-pages": "^1.2.0",
    "node-fetch": "^2.1.2",
    "parcel-bundler": "^1.9.4",
    "parcel-plugin-eslint": "^1.0.4",
    "parcel-plugin-markdown": "^0.3.1",
    "typescript": "^2.9.2"
  }
}
